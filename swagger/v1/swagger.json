    },
    "license": {
      "name": "MIT License",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "paths": {
    "/api/encrypt": {
      "post": {
        "summary": "encrypt string",
        "tags": [
          "String handling"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "input",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "pubkey-id": {
                  "type": "string"
                },
                "message": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "pubkey-id",
                "message"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success",
            "schema": {
              "type": "object",
              "properties": {
                "cipher": {
                  "type": "string"
                }
              },
              "required": [
                "cipher"
              ]
            }
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "description": "Authorization token",
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
}